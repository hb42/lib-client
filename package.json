{
  "name": "@hb42/lib-client",
  "version": "1.1.18",
  "copyright": "Copyright (c) 2018-2020 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "dist/index.js",
  "typings": "./dist",
  "scripts": {
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:dist && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ng build",
    "build": "npm run clean && npm run ngc",
    "pack": "cd dist && npm publish --access=public && cd .. ",
    "make": "npm --no-git-tag-version version prerelease --preid=beta && npm run build && npm run pack",
    "release:patch": "npm version patch && npm run build && npm run pack",
    "release:minor": "npm version minor && npm run build && npm run pack",
    "release:major": "npm version major && npm run build && npm run pack",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "prettier": {
    "printWidth": 100
  },
  "peerDependencies": {
    "@hb42/lib-common": "^1.0.0",
    "rxjs": "^6.5.2",
    "semver": "^7.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.1",
    "@angular-devkit/build-ng-packagr": "^0.901.1",
    "@angular/cli": "^9.1.1",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@angular/platform-browser": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@hb42/lib-common": "^1.0.5",
    "@types/node": "^12.12.35",
    "@types/semver": "^7.0.0",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.2.0",
    "ng-packagr": "^9.1.0",
    "prettier": "^2.0.4",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "semver": "^7.2.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.38.1",
    "tslib": "^1.11.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.8.3",
    "zone.js": "^0.10.3"
  },
  "dependencies": {}
}
