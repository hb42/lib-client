{
  "name": "@hb42/lib-client",
  "version": "1.1.22",
  "copyright": "Copyright (c) 2018-2021 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "dist/index.js",
  "typings": "./dist",
  "scripts": {
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:dist && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ng build",
    "build": "npm run clean && npm run ngc",
    "pack": "cd dist && npm publish --access=public && cd .. ",
    "make": "npm --no-git-tag-version version prerelease --preid=beta && npm run build && npm run pack",
    "release:patch": "npm version patch && npm run build && npm run pack",
    "release:minor": "npm version minor && npm run build && npm run pack",
    "release:major": "npm version major && npm run build && npm run pack",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@hb42/lib-common": "^1.0.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1200.2",
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^12.0.2",
    "@angular/common": "^12.0.2",
    "@angular/compiler": "^12.0.2",
    "@angular/compiler-cli": "^12.0.2",
    "@angular/core": "^12.0.2",
    "@angular/language-service": "^12.0.2",
    "@angular/platform-browser": "^12.0.2",
    "@angular/router": "^12.0.2",
    "@hb42/lib-common": "^1.0.5",
    "@types/node": "^12.20.5",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.23.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^35.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "ng-packagr": "^12.0.0",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "semver": "^7.3.4",
    "tslib": "^2.1.0",
    "typescript": "4.2.4",
    "zone.js": "~0.11.4"
  }
}
