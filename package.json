{
  "name": "@hb42/lib-client",
  "version": "1.1.21",
  "copyright": "Copyright (c) 2018-2021 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "dist/index.js",
  "typings": "./dist",
  "scripts": {
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:dist && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ng build",
    "build": "npm run clean && npm run ngc",
    "pack": "cd dist && npm publish --access=public && cd .. ",
    "make": "npm --no-git-tag-version version prerelease --preid=beta && npm run build && npm run pack",
    "release:patch": "npm version patch && npm run build && npm run pack",
    "release:minor": "npm version minor && npm run build && npm run pack",
    "release:major": "npm version major && npm run build && npm run pack",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@hb42/lib-common": "^1.0.0",
    "semver": "^7.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1100.7",
    "@angular-devkit/build-angular": "^0.1101.1",
    "@angular-devkit/core": "^11.1.1",
    "@angular-eslint/builder": "^1.1.0",
    "@angular-eslint/eslint-plugin": "^1.1.0",
    "@angular-eslint/eslint-plugin-template": "^1.1.0",
    "@angular-eslint/schematics": "^1.1.0",
    "@angular-eslint/template-parser": "^1.1.0",
    "@angular/cli": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/language-service": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/router": "^11.1.1",
    "@hb42/lib-common": "^1.0.5",
    "@types/node": "^12.19.15",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "ng-packagr": "^11.1.2",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "semver": "^7.3.4",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.5",
    "zone.js": "^0.10.3"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
