{
  "name": "@hb42/lib-client",
  "version": "1.1.18",
  "copyright": "Copyright (c) 2018-2020 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "dist/index.js",
  "typings": "./dist",
  "scripts": {
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:dist && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ng build",
    "build": "npm run clean && npm run ngc",
    "pack": "cd dist && npm publish --access=public && cd .. ",
    "make": "npm --no-git-tag-version version prerelease --preid=beta && npm run build && npm run pack",
    "release:patch": "npm version patch && npm run build && npm run pack",
    "release:minor": "npm version minor && npm run build && npm run pack",
    "release:major": "npm version major && npm run build && npm run pack",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@hb42/lib-common": "^1.0.0",
    "rxjs": "^6.5.2",
    "semver": "^7.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular-devkit/build-ng-packagr": "^0.1000.3",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/schematics": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "^10.0.3",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/core": "^10.0.4",
    "@angular/language-service": "^10.0.4",
    "@angular/platform-browser": "^10.0.4",
    "@angular/router": "^10.0.4",
    "@hb42/lib-common": "^1.0.5",
    "@types/node": "^12.12.50",
    "@types/semver": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^24.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "ng-packagr": "^10.0.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "semver": "^7.3.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "zone.js": "^0.10.3"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "lint": {
    "builder": "@angular-eslint/builder:lint",
    "options": {
      "eslintConfig": ".eslintrc.js",
      "tsConfig": [
        "tsconfig.app.json",
        "tsconfig.spec.json",
        "e2e/tsconfig.json"
      ],
      "exclude": ["**/node_modules/**"]
    }
  }
}
