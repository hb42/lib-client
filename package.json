{
  "name": "@hb42/lib-client",
  "version": "1.0.7",
  "copyright": "Copyright (c) 2018-2019 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "lib/src/index.js",
  "typings": "./lib",
  "scripts": {
    "clean:lib": "./node_modules/.bin/rimraf lib",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:lib && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ngc",
    "build": "npm run clean && npm run ngc",
    "commit": "git add -A ./lib && git commit -m \"package\" ./lib",
    "push": "git push && git push --tags",
    "make": "npm run build && npm run commit && npm version prerelease --preid=beta",
    "release": "npm run build && npm run commit && npm version patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@hb42/lib-common": "https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "rxjs": "^6.0.0",
    "semver": "^5.3.0",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@hb42/lib-common": "https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "@types/node": "^10.12.20",
    "@types/semver": "^5.5.0",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "semver": "^5.6.0",
    "tslint": "^5.12.0",
    "typescript": "3.2.4",
    "zone.js": "^0.8.29"
  },
  "dependencies": {}
}
