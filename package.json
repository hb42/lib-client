{
  "name": "@hb42/lib-client",
  "version": "1.0.11",
  "copyright": "Copyright (c) 2018-2019 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "lib/src/index.js",
  "typings": "./lib",
  "scripts": {
    "clean:lib": "./node_modules/.bin/rimraf lib",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:lib && npm run clean:tmp",
    "ngc": "ng build lib-client",
    "build": "npm run clean && npm run ngc",
    "commit": "git add -A ./lib && git commit -m \"publish npm package\" ./lib",
    "make": "npm run build && npm run commit && npm version prerelease --preid=beta",
    "release:patch": "npm run build && npm run commit && npm version patch",
    "release:minor": "npm run build && npm run commit && npm version minor",
    "release:major": "npm run build && npm run commit && npm version major",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@hb42/lib-common": "https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "rxjs": "^6.0.0",
    "semver": "^5.3.0",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@hb42/lib-common": "git+https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.14.6",
    "@types/semver": "^5.5.0",
    "codelyzer": "~4.5.0",
