{
  "name": "@hb42/lib-client",
  "version": "1.1.19",
  "copyright": "Copyright (c) 2018-2020 Hartmut Bauer",
  "description": "Common objects for Angular clients",
  "main": "dist/index.js",
  "typings": "./dist",
  "scripts": {
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:dist && npm run clean:tmp",
    "ngc": "./node_modules/.bin/ng build",
    "build": "npm run clean && npm run ngc",
    "pack": "cd dist && npm publish --access=public && cd .. ",
    "make": "npm --no-git-tag-version version prerelease --preid=beta && npm run build && npm run pack",
    "release:patch": "npm version patch && npm run build && npm run pack",
    "release:minor": "npm version minor && npm run build && npm run pack",
    "release:major": "npm version major && npm run build && npm run pack",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-client",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@hb42/lib-common": "^1.0.0",
    "semver": "^7.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1100.0",
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular-eslint/builder": "^0.7.0-beta.1",
    "@angular-eslint/eslint-plugin": "^0.7.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "^0.7.0-beta.1",
    "@angular-eslint/schematics": "^0.7.0-beta.1",
    "@angular-eslint/template-parser": "^0.7.0-beta.1",
    "@angular/cli": "^11.0.1",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@hb42/lib-common": "^1.0.5",
    "@types/node": "^12.19.4",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "ng-packagr": "^11.0.2",
    "postcss": "^8.1.7",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "semver": "^7.3.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.0",
    "typescript": "~4.0.5",
    "zone.js": "^0.10.3"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
